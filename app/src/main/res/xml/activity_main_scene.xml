<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_editor_absoluteY="56dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/titleImage" />
        <Constraint
            motion:layout_editor_absoluteY="335dp"
            motion:layout_editor_absoluteX="22dp"
            android:id="@+id/password" />
        <Constraint
            motion:layout_editor_absoluteY="213dp"
            motion:layout_editor_absoluteX="22dp"
            android:id="@+id/username" />
        <Constraint
            motion:layout_editor_absoluteY="463dp"
            motion:layout_editor_absoluteX="152dp"
            android:id="@+id/loginButton" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>